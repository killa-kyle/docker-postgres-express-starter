{
  "name": "database-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build+run": "tsc && node dist/src/index.js",
    "debug": "node --inspect --require=ts-node/register src/index.ts",
    "debug-with-docker": "node --inspect=0.0.0.0 --require=ts-node/register src/index.ts",
    "start": "node dist/src/index.js",
    "dev": "nodemon -e ts --exec 'npm run build+run'",
    "dev-with-docker": "nodemon -e ts --exec 'npm run debug-with-docker'",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "dotenv-extended": "^2.9.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3"
  }
}